{
  "name": "minimal-test-environment-threejs npm Runner (Node 22)",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "workspaceFolder": "/workspaces/minimal-test-environment-threejs",
  "remoteUser": "node",
  "runArgs": [
    "--cap-drop=ALL",
    "--cap-add=SYS_ADMIN",
    "--name=minimal-test-env-npm-runner"
  ],
  "postCreateCommand": "npm ci",
  "postStartCommand": "npm audit --audit-level=moderate || true",
  "containerEnv": {
    "NODE_ENV": "development",
    "NPM_CONFIG_AUDIT": "true"
  }
}
